<!-- Start User Area -->
<section class="user-area-style log-in-style ptb-100">
    {% if app.session.flashBag.has('info') %}
        <div class="alert alert-success">
            {% for msg in app.session.flashBag.get('info') %}
                {{ msg }}
            {% endfor %}
        </div>
    {% endif %}
    {% if app.session.flashBag.has('info2') %}
        <div class="alert alert-success">
            {% for msg in app.session.flashBag.get('info2') %}
                {{ msg }}
            {% endfor %}
        </div>
    {% endif %}
    <div class="container">
        <div class="contact-form-action">
            <div class="account-title">
                <h2>Log in</h2>
            </div>
{{ form_start(form) }}
            <form method="post">
                <div class="row">
                    <div class="col-12">
                        <div class="form-group">
                        {#     {{ form_widget(form.privilege) }}
                            <br>
                            <br>
                            <br>#}
                        </div>
                    </div>
                    <div class="col-12">
                        <div class="form-group">

                          {#   <label>Email</label>
                            <input class="form-control" type="text" name="mail">#}
                            {{ form_widget(form.mail) }}
                        </div>
                    </div>

                    <div class="col-12">
                        <div class="form-group">
                            {{ form_widget(form.password) }}
                            {{ form_row(form._token) }}
                        {#     <label>Password</label>
                            <input class="form-control" type="password" name="password">#}

                        </div>
                    </div>
                    <input type="hidden" name="_csrf_token" value="{{ csrf_token('authenticate') }}">
                    <div class="col-12">
                        <div class="login-action">
									<span class="log-rem">
										<input id="remember" type="checkbox">
										<label for="remember">Remember me!</label>
									</span>

                            <span class="forgot-login">
										<a href="{{ path('NVP') }}">Forgot your password?</a>
									</span>
                        </div>
                    </div>

                    <div class="col-12">
                        <button class="default-btn" type="submit">
                            <span>Log In</span>
                        </button>
                    </div>
                </div>
                <input type="hidden" name="form[_token]" value="{{ form._token.vars.value }}" />
                {{ form_rest(form) }}
            </form>
            {{ form_end(form) }}

        </div>
    </div>
</section>
