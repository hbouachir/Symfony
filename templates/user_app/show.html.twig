            <tr>
                <th>Password</th>
                <td>{{ user_app.password }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('user_app_index') }}">back to list</a>

    <a href="{{ path('user_app_edit', {'id': user_app.id}) }}">edit</a>

    {{ include('user_app/_delete_form.html.twig') }}
