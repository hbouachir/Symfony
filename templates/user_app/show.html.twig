{% extends 'base.html.twig' %}

{% block title %}UserApp{% endblock %}

{% block body %}
    <h1>UserApp</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ user_app.id }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ user_app.email }}</td>
            </tr>
            <tr>
                <th>Roles</th>
